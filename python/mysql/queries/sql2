SET SQL_SAFE_UPDATES = 0;

INSERT INTO names (first_name, last_name, email)
VALUES ('John','Johnman','sillyjohnny@happygolucky.bee'), ('Bean', 'Legume', 'fruit@beanpc.com'), ('Rincewind','The Wizard','technicallyawizard@discworld.p');

SELECT * from names;

SELECT * from names WHERE email = "sillyjohnny@happygolucky.bee";

SELECT * from names WHERE id=3;

UPDATE names SET last_name = "Pancakes" WHERE id=3;

DELETE FROM names WHERE id=2;

SELECT * FROM names ORDER BY first_name ASC;

SELECT * FROM names ORDER BY first_name DESC;
