
// Predict 1
function myBirthYearFunc(){
    console.log("I was born in " + 1980);
}
// console.log will state "I was born in 1980"


// Predict 2
function myBirthYearFunc(birthYearInput){
    console.log("I was born in " + birthYearInput);
}
// if birthYearInput is set to 1980, console.log will state "I was born in 1980" (again)


// Predict 3
function add(num1, num2){
    console.log("Summing Numbers!");
    console.log("num1 is: " + num1);
    console.log("num2 is: " + num2);
    var sum = num1 + num2;
    console.log(sum);
}
// console.log will state: "Summing numbers!" "num1 is: 10" "num2 is: 20" "30" (with line breaks between)